package step1;

public class For3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//이중 for 문 2단~9단 구구단 만들기 
		int i,j;
		for(i=2; i<10; i++) {
			for(j=1; j<10; j++) { //9번
				System.out.println(i+"*"+j+"="+i*j);	
			}
			
		}
		
		//배열연습해보기 
		/*int[][] arr = new int[2][3];

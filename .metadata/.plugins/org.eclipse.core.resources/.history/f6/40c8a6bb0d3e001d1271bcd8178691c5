package step1;

public class For5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//备备窜 1窜尝府 2窜尝府...

		int i,j;
		for(i=0; i<10; i++) {
			for(j=2;j<10;j++) {
				if(i==0) {
					System.out.println(j+"窜\t");	
				} else {
					System.out.println(j+"*"+i+"="+(j*i+"\t"));	
				}
			}
			System.out.println();
		}
	}

}

package second;

public class VarTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean a =true;
		byte b =10;
		short c =20;
		
		int d = 30;
		long e = 40;
		
		//한글자만 가능
		char h ='k';
		String i = "hello java";
		
		//소수점
		float f =12.5f;
		//int 정수형에 소수점을 붙이면 빨간줄 오류가 뜬다. 
//		int d = 31.2;
		//소숫점 두개까지 가능
		double g = 34.56;
		
		System.out.ptintln("boolean :"+a);
		System.out.ptintln("byte :"+b);
		System.out.ptintln("short :"+c);
		System.out.ptintln("int :"+d);
		System.out.ptintln("long :"+e);
		System.out.ptintln("floa :t :"+f);
		System.out.ptintln("bouble :"+g);
		System.out.ptintln("char :"+h);
		System.out.ptintln("String :"+i);


		
	}

}
